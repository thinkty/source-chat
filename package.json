{
  "name": "source-chat",
  "version": "1.0.0",
  "description": "A template server for managing intents on Dialogflow using the Dialogflow Editor and also serving as a chatbot for various chat application platforms",
  "main": "app.js",
  "scripts": {
    "start": "./node_modules/.bin/nodemon src/app.js",
    "test": "NODE_ENV=test ./node_modules/.bin/jest",
    "test:cov": "NODE_ENV=test ./node_modules/.bin/jest --coverage",
    "test:watch": "NODE_ENV=test ./node_modules/.bin/jest --watch",
    "test:watchall": "NODE_ENV=test ./node_modules/.bin/jest --watchAll",
    "test:leaks": "NODE_ENV=test ./node_modules/.bin/jest --runInBand --detectOpenHandles",
    "lint": "./node_modules/.bin/eslint src/",
    "lint:fix": "./node_modules/.bin/eslint --fix src/"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/thinkty/source-chat.git"
  },
  "author": "Tae Yoon Kim",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/thinkty/source-chat/issues"
  },
  "homepage": "https://github.com/thinkty/source-chat",
  "dependencies": {
    "@google-cloud/dialogflow": "^3.2.0",
    "axios": "^0.20.0",
    "body-parser": "^1.19.0",
    "cors": "^2.8.5",
    "discord.js": "^12.3.1",
    "dotenv": "^8.2.0",
    "express": "^4.17.1",
    "mongoose": "^5.10.0",
    "morgan": "^1.10.0",
    "uuid": "^8.3.0",
    "winston": "^3.3.3"
  },
  "devDependencies": {
    "eslint": "^7.7.0",
    "eslint-config-airbnb-base": "^14.2.0",
    "eslint-plugin-import": "^2.22.0",
    "jest": "^26.4.2",
    "nodemon": "^2.0.4",
    "supertest": "^4.0.2"
  },
  "jest": {
    "testEnvironment": "node",
    "coveragePathIgnorePatterns": [
      "/node_modules/"
    ]
  }
}
